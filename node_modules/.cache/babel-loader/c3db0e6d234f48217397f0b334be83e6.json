items-center h-96\">\n    <div>\n      <h1 class=\"text-xl\">To-Do App</h1>\n      <div v-if=\"!isEditing\">\n        <label class=\"block\">\n          <span\n            class=\"\n              after:content-['*'] after:ml-0.5 after:text-red-500\n              block\n              text-sm\n              font-medium\n              text-gray-700\n            \"\n          >\n            New To-Do\n          </span>\n          <input\n            type=\"text\"\n            name=\"text\"\n            class=\"\n              mt-1\n              px-3\n              py-2\n              bg-white\n              border\n              shadow-sm\n              border-gray-300\n              placeholder-gray-400\n              focus:outline-none focus:border-sky-500 focus:ring-sky-500\n              block\n              w-full\n              rounded-md\n              sm:text-sm\n              focus:ring-1\n            \"\n            v-model=\"todo\"\n          />\n        </label>\n        <button @click=\"storeData\" class=\"bg-indigo-500 mt-2 px-5 ...\">\n          Add ToDo\n        </button>\n        <button @click=\"clearData\" class=\"bg-indigo-500 mt-2 px-5 ml-9 ...\">\n          Clear All\n        </button>\n      </div>\n\n      <div v-else>\n        <label class=\"block\">\n          <span\n            class=\"\n              after:content-['*'] after:ml-0.5 after:text-red-500\n              block\n              text-sm\n              font-medium\n              text-gray-700\n            \"\n          >\n            New To-Do\n          </span>\n          <input\n            type=\"text\"\n            name=\"text\"\n            class=\"\n              mt-1\n              px-3\n              py-2\n              bg-white\n              border\n              shadow-sm\n              border-gray-300\n              placeholder-gray-400\n              focus:outline-none focus:border-sky-500 focus:ring-sky-500\n              block\n              w-full\n              rounded-md\n              sm:text-sm\n              focus:ring-1\n            \"\n            v-model=\"todo\"\n          />\n        </label>\n        <button @click=\"updateData\" class=\"bg-indigo-500 mt-2 px-5 ...\">\n          Add ToDo\n        </button>\n\n        <clear-todo></clear-todo>\n      </div>\n\n      <div class=\"to-do-list\">\n        <h1 class=\"mt-6 text-xl ...\">To-Do List</h1>\n        <div class=\"border-b-4 border-indigo-500 ...\"></div>\n\n        <ul class=\"list-disc list-inside\">\n          <li v-for=\"(todo, index) in todos\" :key=\"todo\">\n            {{ todo }}\n            <button\n              @click=\"editData(index, todo)\"\n              class=\"bg-red-200 mt-3 px-5 mx-px ...\"\n            >\n              Edit\n            </button>\n            <button\n              @click=\"removeData(index)\"\n              class=\"bg-indigo-200 mt-2 px-5 mx-px ...\"\n            >\n              Remove\n            </button>\n          </li>\n        </ul>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport ClearTodo from \"./ClearTodo.vue\";\n\nexport default {\n  components: {\n    ClearTodo,\n  },\n  name: \"ToDo\",\n  data() {\n    return {\n      index: null,\n      todo: \"\",\n      isEditing: false,\n      todos: [],\n    };\n  },\n  methods: {\n    storeData() {\n      this.todos.push(this.todo);\n      this.todo = \"\";\n    },\n    removeData(index) {\n      this.todos.splice(index, 1);\n    },\n    editData(index, todo) {\n      this.todo = todo;\n      this.index = index;\n      this.isEditing = true;\n    },\n    updateData() {\n      this.todos.splice(this.index, 1, this.todo);\n      this.isEditing = false;\n      this.todo = \"\";\n    },\n    clearData(){\n      this.todos = []\n    }\n  },\n};\n</script>\n"],"sourceRoot":""}]}